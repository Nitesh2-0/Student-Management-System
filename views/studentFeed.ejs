      </div>
      <!-- Navigation Links -->
      <div class="hidden lg:flex lg:space-x-8">
        <a href="#hero" class="text-white hover:text-gray-300">Home</a>
        <a href="#about" class="text-white hover:text-gray-300">About</a>
