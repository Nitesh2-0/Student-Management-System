      color: #fff;
    }

    .hover\:text-gray-300:hover {
      color: #d1d5db;
    }

    .hover\:text-blue-600:hover {
      color: #2563eb;
    }

    .bg-blue-500 {
      background-color: #3b82f6;
    }

